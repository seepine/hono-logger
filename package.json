{
  "name": "@seepine/hono-logger",
  "version": "0.1.1",
  "description": "logger for hono",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "hono",
    "logger",
    "pinia"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seepine/hono-logger.git"
  },
  "homepage": "https://github.com/seepine/hono-logger",
  "author": "seepine (https://github.com/seepine)",
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "format": "prettier --write src *.config.ts"
  },
  "peerDependencies": {
    "hono": ">=4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "~3.0.9",
    "hono": "^4.10.3",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.2",
    "vitest": "~3.0.9"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2"
  }
}